import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    System.out.println("The total money is: ");
    double money = input.nextDouble();
    int toonies, loonies, quarters, dimes, nickels, pennies;
    int other = (int) (money * 100);
    loonies = other / 100;
    System.out.println("The maximun number of loonies is: "+ loonies);
    toonies = other / 200; 
    System.out.println("The maximum number of toonies is: "+ toonies);
    quarters = other / 25;
    System.out.println("The maximum number of quarters is: "+ quarters);
    dimes = other / 10;
    System.out.println("The maximum number of dimes is: "+ dimes);
    nickels = other / 5;
    System.out.println("The maximum number of nickels is: "+ nickels);
    pennies = other / 1;
    System.out.println("The maximum number of pennies is: "+ pennies); 
  }
}
